@inject Session session

@code {
    [Parameter] public List<QSet> qSets { get; set; }
}

<table class="table table-striped table-sm" style="font-size: 14px;">
    <thead>
        <tr>
            <th scope="col">Question</th>
            <th scope="col">Percent</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var qSet in qSets)
        {
            <tr class="tableRow">
                <td>@qSet.Title</td>
                <td>@(qSet.ScorePercent)%</td>
            </tr>
        }
    </tbody>
</table>

<style>
    th {
        font-weight: bold;
    }
</style>